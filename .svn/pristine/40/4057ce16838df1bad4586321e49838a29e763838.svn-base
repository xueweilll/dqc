}

a:link {
	color: #fff;
	text-decoration: none;
	font-size: 12px;
}

a:visited {
	color: #fff;
	text-decoration: none;
}

a:hover {
	color: #fff;
	text-decoration: none;
	cursor: pointer;
	font-weight: bold;
}

a:active {
	color: #CF0000;
}

.xx a {
